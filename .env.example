# RPX XUI test settings
APP_BASE_URL=https://manage-case.aat.platform.hmcts.net
APP_HEALTH_ENDPOINT=/health
APP_API_BASE_URL=https://manage-case.aat.platform.hmcts.net

# IDAM + OAuth2
IDAM_WEB_URL=https://idam-web-public.aat.platform.hmcts.net
IDAM_TESTING_SUPPORT_URL=https://idam-testing-support-api.aat.platform.hmcts.net
IDAM_CLIENT_ID=xuiwebapp
# NEVER commit real client secrets. Inject via CI secret store.
IDAM_SECRET=
IDAM_RETURN_URL=https://manage-case.aat.platform.hmcts.net/oauth2/callback

# Service-to-service gateway
S2S_URL=http://rpe-service-auth-provider-aat.service.core-compute-aat.internal/testing-support/lease
# NEVER commit real S2S secret. Inject via CI secret store.
S2S_SECRET=
S2S_MICROSERVICE_NAME=xui_webapp

# Primary test users (mirror rpx-xui-webapp config)
CASEMANAGER_USERNAME=
CASEMANAGER_PASSWORD=
JUDGE_USERNAME=
JUDGE_PASSWORD=
SOLICITOR_USERNAME=
SOLICITOR_PASSWORD=

# Observability toggles
LOG_LEVEL=info
LOG_FORMAT=json
LOG_REDACTION=on
PLAYWRIGHT_DEFAULT_REPORTER=list
PLAYWRIGHT_JUNIT_OUTPUT=test-results/junit/results.xml
PLAYWRIGHT_DEBUG_API=0
STAFF_SEARCH_ENABLED=true
CASE_FLAGS_CASE_ID=0000000000000000 # example placeholder
CASE_FLAGS_ENABLED=false
PRL_CASE_TABS_CASE_ID=

# COURTNAV (sample placeholders)
COURTNAV_SECRET=
COURTNAV_USERNAME=
COURTNAV_PASSWORD=
COURTNAV_SUBSCRIPTION_KEY_CREATE_CASE=
COURTNAV_CASE_URL=https://cft-api-mgmt.aat.platform.hmcts.net/prl-cos-api/case
COURTNAV_DOC_URL=https://cft-api-mgmt.aat.platform.hmcts.net/prl-document-api/
COURTNAV_SUBSCRIPTION_KEY_ADD_DOC=

