{
  "environment": "aat",
  "urls": {
    "xui": "https://manage-case.aat.platform.hmcts.net",
    "api": "https://manage-case.aat.platform.hmcts.net"
  },
  "test": {
    "TRI": "unset",
    "projectName": "rpx-xui-e2e-tests",
    "timeout": 60000,
    "expectTimeout": 30000,
    "retries": 0,
    "workers": 8,
    "apiRequestTimeout": 10000,
    "maxWaitForHealthy": 60,
    "navigationRetries": 3,
    "accessibility": {
      "autoscanEnabled": true,
      "axeTags": "wcag21a,best-practice"
    },
    "wiremock": {
      "enabled": false,
      "baseUrl": "http://localhost:8888",
      "autoTearDownMappings": true,
      "endpoints": {
        "health": "/__admin/health"
      }
    },
    "reporting": {
      "html": {
        "outputFolder": "playwright-report"
      },
      "junit": {
        "outputFile": "playwright-junit.xml"
      },
      "odhin": {
        "outputFolder": "test-results/odhin-report",
        "index": "playwright-odhin.html"
      }
    }
  },
  "ui": {
    "sessionDir": ".sessions",
    "defaultUserKey": "default"
  },
  "users": {
    "aat": {},
    "demo": {}
  }
}
